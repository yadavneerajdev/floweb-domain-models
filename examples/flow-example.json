{
  "id": "flow-123",
  "name": "Login and Navigate Flow",
  "description": "Automated login flow for testing purposes",
  "tags": ["login", "authentication", "test"],
  "parentID": "folder-auth",
  "type": "flow",
  "actions": [
    {
      "id": "action-1",
      "type": "navigate",
      "position": { "x": 100, "y": 100 },
      "data": {
        "label": "Navigate to Login Page",
        "type": "navigate",
        "config": {
          "url": "https://example.com/login",
          "waitForLoad": true,
          "takeScreenshot": false
        }
      }
    },
    {
      "id": "action-2",
      "type": "input",
      "position": { "x": 300, "y": 100 },
      "data": {
        "label": "Enter Username",
        "type": "input",
        "config": {
          "selector": "#username",
          "text": "{{username}}",
          "clearFirst": true,
          "waitForElement": true,
          "scrollIntoView": true
        }
      }
    },
    {
      "id": "action-3",
      "type": "input",
      "position": { "x": 500, "y": 100 },
      "data": {
        "label": "Enter Password",
        "type": "input",
        "config": {
          "selector": "#password",
          "text": "{{password}}",
          "clearFirst": true,
          "waitForElement": true,
          "scrollIntoView": true
        }
      }
    },
    {
      "id": "action-4",
      "type": "click",
      "position": { "x": 700, "y": 100 },
      "data": {
        "label": "Click Login Button",
        "type": "click",
        "config": {
          "selector": "#login-button",
          "waitTime": 1000,
          "forceClick": false,
          "scrollIntoView": true,
          "waitForElement": true,
          "clickType": "left",
          "clickCount": 1
        }
      }
    },
    {
      "id": "action-5",
      "type": "wait",
      "position": { "x": 900, "y": 100 },
      "data": {
        "label": "Wait for Dashboard",
        "type": "wait",
        "config": {
          "waitType": "element",
          "selector": "#dashboard",
          "timeout": 10000,
          "waitForInteractable": false,
          "fallbackToDuration": true,
          "fallbackDuration": 2000
        }
      }
    }
  ],
  "edges": [
    {
      "id": "edge-1",
      "source": "action-1",
      "target": "action-2",
      "type": "step"
    },
    {
      "id": "edge-2",
      "source": "action-2",
      "target": "action-3",
      "type": "step"
    },
    {
      "id": "edge-3",
      "source": "action-3",
      "target": "action-4",
      "type": "step"
    },
    {
      "id": "edge-4",
      "source": "action-4",
      "target": "action-5",
      "type": "step"
    }
  ],
  "zoom": {
    "x": 0,
    "y": 0,
    "zoom": 1
  },
  "createdAt": "2024-01-15T10:30:00Z",
  "updatedAt": "2024-01-15T14:45:00Z",
  "variables": {
    "input": [
      {
        "id": "var-1",
        "name": "username",
        "type": "string",
        "value": "testuser@example.com",
        "description": "Login username"
      },
      {
        "id": "var-2",
        "name": "password",
        "type": "string",
        "value": "securepassword123",
        "description": "Login password"
      }
    ],
    "output": [
      {
        "id": "var-3",
        "name": "loginSuccess",
        "type": "boolean",
        "value": "true",
        "description": "Whether login was successful"
      }
    ]
  },
  "parameters": {
    "input": [
      {
        "id": "param-1",
        "name": "environment",
        "type": "string",
        "value": "staging",
        "description": "Target environment"
      }
    ],
    "output": []
  },
  "environment": {
    "id": "env-staging",
    "name": "Staging Environment",
    "variables": [
      {
        "id": "env-var-1",
        "name": "baseUrl",
        "type": "url",
        "value": "https://staging.example.com"
      }
    ],
    "description": "Staging environment configuration",
    "isActive": true
  },
  "globalVariables": [
    {
      "id": "global-var-1",
      "name": "timeout",
      "type": "number",
      "value": "30000",
      "description": "Default timeout in milliseconds"
    }
  ],
  "synced": true,
  "lastRun": "2024-01-15T14:45:00Z",
  "lastResult": "passed"
}