{
  "name": "@floweb/domain-models",
  "version": "1.0.0",
  "description": "Domain models for Floweb Desktop - centralized type definitions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "generate": "node generate-types.js",
    "generate-python": "node generate-python-models.js",
    "validate": "ajv validate -s schemas/*.json -d examples/*.json",
    "test": "jest",
    "docs": "typedoc",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "floweb",
    "domain-models",
    "typescript",
    "automation",
    "testing",
    "schemas"
  ],
  "author": "Floweb Team",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/floweb/floweb-domain-models.git"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "ajv-cli": "^5.0.0",
    "jest": "^29.0.0",
    "json-schema-to-typescript": "^13.0.0",
    "ts-jest": "^29.4.6",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "files": [
    "dist/**/*",
    "types/**/*",
    "schemas/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
